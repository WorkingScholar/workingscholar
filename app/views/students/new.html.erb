<div class="container">
  <div class="row">
    <h5>Complete your profile</h5>
  </div>

  <div class="row">
    <div class="col s12">
      <div class="section">
        <%= form_for @student,
          html: { method: :post } do |f| %>
            <%= render "form", f: f, student: @student, button_text: "Complete" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
